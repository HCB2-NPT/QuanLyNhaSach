<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:UserControls="clr-namespace:QuanLyNhaSach.Window_Handlers.MainWindow.Functions.UserControls"
	mc:Ignorable="d"
	x:Class="QuanLyNhaSach.tabQuanLyPhieuNhap"
	x:Name="UserControl"
	d:DesignWidth="600" d:DesignHeight="600" Background="LightGray">
	
	<DockPanel>
		<DockPanel Margin="8,4" DockPanel.Dock="Top" Height="48" Background="White">
			<Button Content="Button" Height="24" VerticalAlignment="Center" Width="24" DockPanel.Dock="Right" Template="{DynamicResource Button_Icon_Template}" BorderBrush="{x:Null}" BorderThickness="0" Foreground="{x:Null}" HorizontalAlignment="Center" Margin="4">
				<Button.Background>
					<ImageBrush ImageSource="/Tiki Bookshop;component/Assets/Images/Search-64.png"/>
				</Button.Background>
			</Button>
			<TextBox TextWrapping="Wrap" ToolTip="Mã Phiếu Nhập" VerticalAlignment="Center" Margin="16,0,0,0" Style="{DynamicResource TextBoxListViewItem}"/>
		</DockPanel>
		<DockPanel DockPanel.Dock="Top" Height="120" Margin="8,4" Background="White">
			<ListView>
			</ListView>
		</DockPanel>
		<DockPanel Margin="0,4,0,8" Width="584" Background="White">
			<ListView DockPanel.Dock="Top"/>
		</DockPanel>
	</DockPanel>
	
	</UserControl>